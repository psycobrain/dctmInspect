        </rowConstraints>
         <children>
            <Label alignment="CENTER_RIGHT" text="Server" GridPane.columnIndex="0" GridPane.rowIndex="0" />
            <Label alignment="CENTER_RIGHT" text="Porta" GridPane.columnIndex="0" GridPane.rowIndex="1" />
            <TextField fx:id="docbrokerServerField" GridPane.columnIndex="1" GridPane.rowIndex="0" />
