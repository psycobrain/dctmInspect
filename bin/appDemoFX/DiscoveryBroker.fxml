            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Label>
      <TableView layoutX="6.0" layoutY="133.0" prefHeight="127.0" prefWidth="389.0">
        <columns>
          <TableColumn prefWidth="75.0" text="Server" />
          <TableColumn prefWidth="75.0" text="Porta" />
        </columns>
      </TableView>
      <Button layoutX="334.0" layoutY="260.0" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
      <Button layoutX="269.0" layoutY="261.0" mnemonicParsing="false" onAction="#handleOk" prefHeight="25.0" prefWidth="52.0" text="Ok" />
